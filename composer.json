{
  "name": "stuartapp/stuart-client-php",
  "description": "Stuart PHP client library",
  "homepage": "https://github.com/StuartApp/stuart-client-php/",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": "5.6.*",
    "guzzlehttp/guzzle": "^6.2",
    "league/oauth2-client": "^2.1",
    "desarrolla2/cache":  "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8 || ^5.0",
    "phake/phake": "@stable",
    "php-vcr/phpunit-testlistener-vcr": "^2.0",
    "php-vcr/php-vcr": "^1.3"
  },
  "autoload": {
    "psr-4": {
      "Stuart\\": "src/Stuart",
      "Stuart\\Tests\\": "tests/Stuart/tests"
    }
  }
}
